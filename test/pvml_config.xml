<?xml version="1.0"?>
<pvmlConfig>
  <name>Biomass</name>
  <taskTablePath>tasktables</taskTablePath>
  <workspaceDirectory>workspace</workspaceDirectory>
  <taskTableSchema>test/tasktable.xsd</taskTableSchema>
  <jobOrderSchema>test/joborder_new.xsd</jobOrderSchema>
  <ProcessorInterfaceVersion>1</ProcessorInterfaceVersion>
  <processingStation>GAT</processingStation>  <!-- This value will be used to populate the corresponding entry in the JobOrder file.-->
  <splitLoggingLevel>false</splitLoggingLevel>
  <globalBreakpointEnable>false</globalBreakpointEnable>
  <alternateBreakpointElementName>true</alternateBreakpointElementName>
  <useTroubleshooting>true</useTroubleshooting>
  <listFilenameUsesOrderId>false</listFilenameUsesOrderId>

  <outputBackend>file</outputBackend>
  <outputBackendOptions>
    <option name="archive">archive</option>
  </outputBackendOptions>
  <productTypes>
    <!-- For each product type that is included in a TaskTable where `Destination=DB`, there should be an entry -->
    <!-- Note: the productType names are file_types in the task tables! Consider renaming...-->
    <!-- Raw -->
    <productType name="RAW_022_10">
      <matchExpression><![CDATA[.*RAW_022_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_023_10">
      <matchExpression><![CDATA[.*RAW_023_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_024_10">
      <matchExpression><![CDATA[.*RAW_024_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_025_10">
      <matchExpression><![CDATA[.*RAW_025_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_026_10">
      <matchExpression><![CDATA[.*RAW_026_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_027_10">
      <matchExpression><![CDATA[.*RAW_027_10.*]]></matchExpression>
    </productType>
    <productType name="RAW_028_10">
      <matchExpression><![CDATA[.*RAW_028_10.*]]></matchExpression>
    </productType>

    <productType name="RAWS025_10">
      <matchExpression><![CDATA[.*RAWS025_10.*]]></matchExpression>
    </productType>
    <productType name="RAWS026_10">
      <matchExpression><![CDATA[.*RAWS026_10.*]]></matchExpression>
    </productType>
    <productType name="RAWS027_10">
      <matchExpression><![CDATA[.*RAWS027_10.*]]></matchExpression>
    </productType>
    <productType name="RAWS028_10">
      <matchExpression><![CDATA[.*RAWS028_10.*]]></matchExpression>
    </productType>

    <productType name="RAWS022_10">
      <matchExpression><![CDATA[.*RAWS022_10.*]]></matchExpression>
    </productType>
    <productType name="RAWS023_10">
      <matchExpression><![CDATA[.*RAWS023_10.*]]></matchExpression>
    </productType>
    <productType name="RAWS024_10">
      <matchExpression><![CDATA[.*RAWS024_10.*]]></matchExpression>
    </productType>

    <!-- TODO: does this replace all discrete types? -->
    <productType name="RAWSxxx_10">
      <matchExpression><![CDATA[.*RAWS0[2-3][0-9]_10.*]]></matchExpression>
    </productType>

    <productType name="RAW___HKTM">
      <matchExpression><![CDATA[.*RAW___HKTM.*]]></matchExpression>
    </productType>

    <!-- Level 0 -->
    <productType name="S1_RAW__0S">
      <matchExpression><![CDATA[.*S[1-3]_RAW__[0-9]S.*]]></matchExpression>
    </productType>
    <productType name="S._RAWP_0M">
      <matchExpression><![CDATA[.*S[1-3]_RAWP_[0-9]S.*]]></matchExpression>
    </productType>
    <productType name="S1_RAW__0M">
      <matchExpression><![CDATA[.*S[1-3]_RAW__[0-9]S.*]]></matchExpression>
    </productType>

  </productTypes>
</pvmlConfig>
